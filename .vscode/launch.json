{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug a.out with arguments",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/cachelab-handout/a.out",
      "args": ["-s", "4", "-E", "1", "-b", "4", "-t", "${workspaceFolder}/cachelab-handout/traces/long.trace"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
